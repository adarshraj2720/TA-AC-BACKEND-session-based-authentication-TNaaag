<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Document</title>
</head>
<body>
    <h1>Product</h1>
    <%products.forEach((product)=>{ %>
    <p>Title: <%=product.title%></p>
    <p>Quantity: <%=product.quantity%></p>
    <p>Price: <%=product.price%></p>
    <p>Like: <%=product.likes%></p>
    <% if(isAdmin){ %>
        <a href="/product/<%=product.id%>/edit"><button>EDIT</button></a>
        <a href="/product/<%=product.id%>/delete"><button>Delete</button></a>
        
    <% } %>
    <a href="/product/<%=product.id%>/like"><button>Likes</button></a>
    <a href="/product/<%=product.id%>/dislike"><button>Dislikes</button></a><br>
    <%})%>

    <a href="/users/logout"><button>LogOut</button></a>
</body>
</html>